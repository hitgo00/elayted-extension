{
  "name": "Elayt-Ed",
  "description": "Search fast, search deep!",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "all_frames": true,
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
